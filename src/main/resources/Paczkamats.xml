<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
        version="3.0">

    <entity class="entities.Paczkamat">
        <table name="paczkamats" schema="paczkamatDB" catalog=""/>
        <attributes>
            <id name="name">
                <column name="name"/>
            </id>
            <basic name="buildingNumber">
                <column name="building_number"/>
            </basic>
            <basic name="city">
                <column name="city"/>
            </basic>
            <basic name="postCode">
                <column name="post_code"/>
            </basic>
            <basic name="province">
                <column name="province"/>
            </basic>
            <basic name="street">
                <column name="street"/>
            </basic>
            <basic name="latitude">
                <column name="latitude"/>
            </basic>
            <basic name="longitude">
                <column name="longitude"/>
            </basic>
            <basic name="openingHours">
                <column name="opening_hours"/>
            </basic>
            <one-to-many name="stash" mapped-by="paczkamat" target-entity="entities.Stash"/>
        </attributes>
    </entity>
</entity-mappings>
