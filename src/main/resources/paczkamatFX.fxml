<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<TabPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PaczkamatController">
   <tabs>
      <Tab id="loginTab" fx:id="loginTab" closable="false" text="Login">
         <content>
            <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
               <center>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                     <children>
                        <TextArea fx:id="loginTextArea" editable="false" focusTraversable="false" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="375.0" />
                        <TextArea fx:id="loginConsoleLog" editable="false" focusTraversable="false" layoutX="14.0" layoutY="253.0" prefHeight="92.0" prefWidth="578.0" />
                     </children>
                  </AnchorPane>
               </center>
               <top>
                  <HBox prefHeight="9.0" prefWidth="600.0" BorderPane.alignment="CENTER">
                     <children>
                        <TextField fx:id="loginLoginField" maxWidth="-Infinity" prefHeight="26.0" prefWidth="100.0">
                           <HBox.margin>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </HBox.margin>
                        </TextField>
                        <PasswordField fx:id="loginPasswordField" prefHeight="26.0" prefWidth="100.0">
                           <HBox.margin>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </HBox.margin>
                        </PasswordField>
                        <Button fx:id="loginButtonDBLogin" mnemonicParsing="false" onAction="#onLoginClicked" text="DB Login">
                           <HBox.margin>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="loginButtonCustomerLogin" layoutX="250.0" layoutY="18.0" mnemonicParsing="false" onAction="#onUserLoginClicked" text="Customer Login">
                           <HBox.margin>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </top>
            </BorderPane>
         </content>
      </Tab>
      <Tab id="sendTab" fx:id="sendTab" closable="false" text="Send">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
      <Tab id="statusTab" fx:id="statusTab" closable="false" text="Status Check">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="StatusCheckPckgNumberInput" layoutX="9.0" layoutY="14.0" prefHeight="25.0" prefWidth="489.0" promptText="Please input your package's tracking number" />
                  <Button fx:id="StatusCheckCheckButton" layoutX="506.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="82.0" text="Check" />
                  <TextArea fx:id="StatusCheckTextArea" editable="false" layoutX="15.0" layoutY="47.0" prefHeight="339.0" prefWidth="570.0" />
               </children></AnchorPane>
        </content>
      </Tab>
      <Tab id="adminTab" fx:id="adminTab" closable="false" text="Admin">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
   </tabs>
</TabPane>
